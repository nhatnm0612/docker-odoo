<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="facebook_redirect" model="website.rewrite">
        <field name="name">Facebook</field>
        <field name="redirect_type">302</field>
        <field name="url_from">/website/social/facebook</field>
        <field name="url_to">https://facebook.com</field>
    </record>

    <record id="youtube_redirect" model="website.rewrite">
        <field name="name">Youtube</field>
        <field name="redirect_type">302</field>
        <field name="url_from">/website/social/youtube</field>
        <field name="url_to">https://youtube.com</field>
    </record>

</odoo>
